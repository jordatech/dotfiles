#!/usr/bin/env bash
#Author:    Jordan Ulmer
#Date:      2017-01-04
#Purpose:   Reset permissions to all files and folders
#Source:    http://stackoverflow.com/a/1580644


find . -t d -exec chmod a+rwx \; # Make folders traversable and read/write
find . -t f -exec chmod a+rw \;  # Make files read/write
